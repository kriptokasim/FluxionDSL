# Vars: oast, token
let t = token ?? "0"
let path = "/fluxion/combo"

# DNS + HTTP
let b = oast_beacon sub=t domain=oast path=path q={ tool: "fluxion", mode: "combo", token: t }

# DÃ¼z HTTP HEAD (command-style named args ile)
let url = b?.url ?? join("http://", t, ".", oast, path, "?tool=fluxion&mode=combo&token=", t)
let h = http_head url=url timeout=5.0

echo value=jsonify({
  token: t,
  beacon: b,
  http: h
})

return (b?.status ?? 0)
